{
  "config": {

    "secure-http": false
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.12",
    "elasticsearch/elasticsearch": "~6.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "ccsd/library",
        "version": "1",
        "source": {
          "url": "https://gitlab.ccsd.cnrs.fr/ccsd/library.git",
          "type": "git",
          "reference": "preprod"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "swordapp/swordappv2",
        "version": "1",
        "source": {
          "url": "https://github.com/swordapp/swordappv2-php-library.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "java/pdfbox",
        "version": "1.8.16",
        "dist": {
          "url": "https://archive.apache.org/dist/pdfbox/1.8.16/pdfbox-app-1.8.16.jar",
          "type": "file"
        }
      }
    }
  ],

  "require": {
    "ccsd/library": "*",
    "swordapp/swordappv2": "*",
    "solarium/solarium": "*",
    "shardj/zf1-future": "^1.19",
    "shardj/zf1-extras-future": "^1.12",
    "apereo/phpcas": "^1.3.5",
    "doctrine/orm": "*",
    "php": ">=7.0",
    "ext-imagick": "*",
    "ext-dom": "*",
    "ext-libxml" : "*",
    "ext-bz2": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-readline": "*",
    "ext-soap": "*",
    "ext-xml": "*",
    "ext-simplexml": "*",
    "ext-zip": "*",
    "ext-intl": "*",
    "ext-geoip": "*",
    "ext-ssh2": "*",
    "ext-posix": "*",
    "java/pdfbox": "*",
    "ezyang/htmlpurifier": "^4.13"
  },
  "autoload": {
    "classmap": [
      "vendor/ccsd/library/",
      "vendor/swordapp/swordappv2/swordappclient.php"
    ]
  }
}
